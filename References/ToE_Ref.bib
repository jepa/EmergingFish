@article{Pinsky:2020jx,
author = {Pinsky, M L and Rogers, L A and Morley, J W and Fr{\"o}licher, T L},
title = {{Ocean planning for species on the move provides substantial benefits and requires few trade-offs}},
journal = {Science Advances},
year = {2020},
volume = {6},
number = {50},
pages = {eabb8428},
month = dec,
publisher = {American Association for the Advancement of Science},
affiliation = {Department of Ecology, Evolution, and Natural Resources, Rutgers University, The State University of New Jersey, 14 College Farm Rd., New Brunswick, NJ 08901, USA. malin.pinsky@rutgers.edu.},
doi = {10.1126/sciadv.abb8428},
pmid = {33310845},
pmcid = {PMC7732182},
language = {English},
rating = {0},
date-added = {2021-02-10T15:00:58GMT},
date-modified = {2021-03-10T14:23:35GMT},
abstract = {<p>Societies increasingly use multisector ocean planning as a tool to mitigate conflicts over space in the sea, but such plans can be highly sensitive to species redistribution driven by climate change or other factors. A key uncertainty is whether planning ahead for future species redistributions imposes high opportunity costs and sharp trade-offs against current ocean plans. Here, we use more than 10,000 projections for marine animals around North America to test the impact of climate-driven species redistributions on the ability of ocean plans to meet their goals. We show that planning for redistributions can substantially reduce exposure to risks from climate change with little additional area set aside and with few trade-offs against current ocean plan effectiveness. Networks of management areas are a key strategy. While climate change will severely disrupt many human activities, we find a strong benefit to proactively planning for long-term ocean change.</p>},
url = {https://advances.sciencemag.org/lookup/doi/10.1126/sciadv.abb8428},
uri = {\url{papers3://publication/doi/10.1126/sciadv.abb8428}}
}

