@article{Frolicher:2020cc,
author = {Fr{\"o}licher, Thomas L and Ramseyer, Luca and Raible, Christoph C and Rodgers, Keith B and Dunne, John},
title = {{Potential predictability of marine ecosystem drivers}},
journal = {Biogeosciences},
year = {2020},
volume = {17},
number = {7},
pages = {2061--2083},
publisher = {Copernicus GmbH},
doi = {10.5194/bg-17-2061-2020},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-04-17T12:52:35GMT},
date-modified = {2020-12-14T23:40:03GMT},
abstract = {<p><strong>Abstract.</strong> Climate variations can have profound impacts on marine ecosystems and the socioeconomic systems that may depend upon them. Temperature, pH, oxygen (<span class="inline-formula">O<sub>2</sub></span>) and net primary production (NPP) are commonly considered to be important marine ecosystem drivers, but the potential predictability of these drivers is largely unknown. Here, we use a comprehensive Earth system model within a perfect modeling framework to show that all four ecosystem drivers are potentially predictable on global scales and at the surface up to 3~years in advance. However, there are distinct regional differences in the potential predictability of these drivers. Maximum potential predictability (<span class="inline-formula">{\&}gt;10</span>~years) is found at the surface for temperature and <span class="inline-formula">O<sub>2</sub></span> in the Southern Ocean and for temperature, <span class="inline-formula">O<sub>2</sub></span> and pH in the North Atlantic. This is tied to ocean overturning structures with {\textquotedblleft}memory{\textquotedblright} or inertia with enhanced predictability in winter. Additionally, these four drivers are highly potentially predictable in the Arctic Ocean at the surface. In contrast, minimum predictability is simulated for NPP (<span class="inline-formula">{\&}lt;1</span>~years) in the Southern Ocean. Potential predictability for temperature, <span class="inline-formula">O<sub>2</sub></span> and pH increases with depth below the thermocline to more than 10~years, except in the tropical Pacific and Indian oceans, where predictability is also 3 to 5 years in the thermocline. This study indicating multi-year (at surface) and decadal (subsurface) potential predictability for multiple ecosystem drivers is intended as a foundation to foster broader community efforts in developing new predictions of marine ecosystem drivers.</p>},
url = {https://www.biogeosciences.net/17/2061/2020/},
local-url = {file://localhost/Users/carmelia/Dropbox/Papers.Library3.Library/Files/2F/2FBA0149-C623-42F9-97F6-CD3B07BADB42.pdf},
file = {{2FBA0149-C623-42F9-97F6-CD3B07BADB42.pdf:/Users/carmelia/Dropbox/Papers.Library3.Library/Files/2F/2FBA0149-C623-42F9-97F6-CD3B07BADB42.pdf:application/pdf;2FBA0149-C623-42F9-97F6-CD3B07BADB42.pdf:/Users/carmelia/Dropbox/Papers.Library3.Library/Files/2F/2FBA0149-C623-42F9-97F6-CD3B07BADB42.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.5194/bg-17-2061-2020}}
}

@article{Ramesh:2019va,
author = {Ramesh, Nandini and Rising, James A and Oremus, Kimberly L},
