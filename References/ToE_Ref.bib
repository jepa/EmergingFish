uri = {\url{papers3://publication/doi/10.1126/science.aai9214}}
}

@article{Roemmich:2015id,
author = {Roemmich, Dean and Church, John and Gilson, John and Monselesan, Didier and Sutton, Philip and Wijffels, Susan},
title = {{Unabated planetary warming and its ocean structure since 2006}},
journal = {Nature Climate Change},
year = {2015},
volume = {5},
number = {3},
pages = {240--245},
month = feb,
publisher = {Nature Publishing Group},
doi = {10.1038/nclimate2513},
language = {English},
rating = {0},
date-added = {2020-09-18T18:17:52GMT},
date-modified = {2020-10-05T16:25:12GMT},
abstract = {Despite the {\textquoteleft}pause{\textquoteright} in surface warming, results from the global Argo programme (2006{\textendash}present) show that the heat content of the oceans is increasing. Increasing heat content of the global ocean dominates the energy imbalance in the climate system1. Here we show that ocean heat gain over the 0{\textendash}2,000 m layer continued at a rate of 0.4{\textendash}0.6 W m{\textminus}2 during 2006{\textendash}2013. The depth dependence and spatial structure of temperature changes are described on the basis of the Argo Program's2 accurate and spatially homogeneous data set, through comparison of three Argo-only analyses. Heat gain was divided equally between upper ocean, 0{\textendash}500 m and 500{\textendash}2,000 m components. Surface temperature and upper 100 m heat content tracked interannual El Ni{\~n}o/Southern Oscillation fluctuations3, but were offset by opposing variability from 100{\textendash}500 m. The net 0{\textendash}500 m global average temperature warmed by 0.005 {\textdegree}C yr{\textminus}1. Between 500 and 2,000 m steadier warming averaged 0.002 {\textdegree}C yr{\textminus}1 with a broad intermediate-depth maximum between 700 and 1,400 m. Most of the heat gain (67 to 98%) occurred in the Southern Hemisphere extratropical ocean. Although this hemispheric asymmetry is consistent with inhomogeneity of radiative forcing4 and the greater area of the Southern Hemisphere ocean, ocean dynamics also influence regional patterns of heat gain.},
url = {http://www.nature.com/articles/nclimate2513},
uri = {\url{papers3://publication/doi/10.1038/nclimate2513}}
}

